{
    "version": 2,
    "builds": [
      {
        "src": "next.config.mjs",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ],
    "env": {
      "NEXT_PUBLIC_ASSEMBLYAI_API_KEY": "@assemblyai_api_key",
      "NEXT_PUBLIC_GETSTREAM_API_KEY": "@getstream_api_key",
      "NEXT_PUBLIC_GETSTREAM_API_SECRET": "@getstream_api_secret"
    }
  }
  